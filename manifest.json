{
    "name": "TF's Center",
    "short_name": "TFs Center",
    "manifest_version": 2,
    "author": "Victor de la Fouchardiere",
    "background": {
        "scripts": [
            "background.bundle.js"
        ]
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon16.png",
        "default_title": "TFs Center"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "content_script.bundle.js"
            ],
            "css": [
                "styles.css"
            ]
        }
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}