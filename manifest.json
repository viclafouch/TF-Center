{
    "manifest_version": 2,
    "name": "TFs Center",
    "description": "A Chrome extension that remakes a new design for the TF tools.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "src/build/main.js"
            ],
            "css": [
                "src/build/styles.css"
            ]
        }
    ]
}