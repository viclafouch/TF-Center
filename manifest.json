{
    "manifest_version": 2,
    "name": "Name of your chrome extension",
    "description": "Description of your chrome extension.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "src/build/main.js"
            ]
        }
    ]
}