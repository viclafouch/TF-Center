{
  "name": "tfs-center",
  "version": "0.1.0",
  "private": true,
  "main": "src/js/main.js",
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build": "parcel build src/js/main.js -d src/build/ -o main.js",
    "watch": "npm-run-all --parallel watch-js watch-scss",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "watch-js": "parcel watch src/js/main.js -d src/build/ -o main.js",
    "watch-scss": "parcel watch src/css/styles.scss -d src/build/ -o styles.css"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
